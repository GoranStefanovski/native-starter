{
  "name": "vue-laravel-starter",
  "description": "A Single Page Application boilerplate using Laravel 5.7, Vue.js 2.5, Bootstrap 4, TypeScript, Sass and Jest.",
  "authors": [
    {
      "name": "Ratko Nikolovski",
      "email": "nikolovski.ratko@gmail.com",
      "homepage": "https://nikolovski.org"
    },
    {
      "name": "The Laravel Framework",
      "homepage": "https://laravel.com"
    },
    {
      "name": "The Vue.js Framework",
      "homepage": "https://vuejs.org/"
    },
    {
      "name": "Bootstrap",
      "homepage": "https://getbootstrap.com/"
    }
  ],
  "keywords": [
    "laravel",
    "vue",
    "boilerplate",
    "typescript",
    "sass",
    "scss",
    "pug",
    "jest"
  ],
  "license": "MIT",
  "scripts": {
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "production": "mix --production",
    "eslint": "./node_modules/.bin/eslint resources/; exit 0",
    "eslint:fix": "./node_modules/.bin/eslint --fix resources/; exit 0"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "**/*.vue",
    "./resources/assets/vue/utils/i18n.ts",
    "./resources/assets/vue/api/axios.ts"
  ],
  "dependencies": {
    "@braid/vue-formulate": "^2.5.3",
    "@ckeditor/ckeditor5-alignment": "^10.0.4",
    "@ckeditor/ckeditor5-build-classic": "^11.2.0",
    "@ckeditor/ckeditor5-vue": "^1.0.0-beta.1",
    "@websanova/vue-auth": "^4.2.1",
    "axios": "^0.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "4.6.0",
    "bootstrap-vue": "^2.21.2",
    "date-fns": "^2.9.0",
    "downloadjs": "^1.4.7",
    "font-awesome": "^4.7.0",
    "form-backend-validation": "https://github.com/JovanDimovski/form-backend-validation",
    "hooper": "^0.3.4",
    "inversify": "^5.0.1",
    "lodash": "^4.17.11",
    "owl.carousel": "^2.3.4",
    "papaparse": "^4.6.3",
    "popper.js": "^1.14.7",
    "reflect-metadata": "^0.1.13",
    "throttle-debounce": "^2.1.0",
    "vue": "^2.7.14",
    "vue-axios": "^2.1.4",
    "vue-click-outside": "^1.0.7",
    "vue-gtm": "^2.0.0",
    "vue-headful": "^2.0.1",
    "vue-i18n": "^9.2.2",
    "vue-lazyload": "^1.3.3",
    "vue-modal-dialogs": "^2.0.0",
    "vue-multiselect": "^2.1.3",
    "vue-programmatic-invisible-google-recaptcha": "^2.0.1",
    "vue-router": "^3.6.*",
    "vue-search-select": "^2.8.2",
    "vue-select": "^3.20.0",
    "vue-smooth-dnd": "^0.8.1",
    "vue-tiny-slider": "^0.1.39",
    "vuejs-datepicker": "^1.5.4",
    "vuex": "^3.1.0",
    "vuex-i18n": "^1.10.0",
    "vuex-map-fields": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@types/grecaptcha": "^2.0.36",
    "@types/jquery": "*",
    "@types/node": "^10.9.4",
    "@types/vuex-i18n": "^1.10.0",
    "@typescript-eslint/parser": "^5.36.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.0.1",
    "core-js": "2",
    "cross-env": "^5.2.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.4.0",
    "glob-all": "^3.1.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "laravel-mix": "^6.0.49",
    "laravel-mix-purgecss": "^6.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "pretty-data": "^0.40.0",
    "purgecss-webpack-plugin": "^4.1.3",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "resolve-url-loader": "2.3.1",
    "sass": "^1.17.0",
    "sass-loader": "13.0.2",
    "sass-resources-loader": "^2.2.5",
    "sitemap-webpack-plugin": "^0.6.0",
    "speed-measure-webpack-plugin": "^1.3.0",
    "style-loader": "^3.3.1",
    "style-resources-loader": "^1.5.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3",
    "vue-class-component": "^6.3.2",
    "vue-eslint-parser": "^9.0.3",
    "vue-loader": "^15.10.0",
    "vue-property-decorator": "^7.3.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.14",
    "vuex-class": "^0.3.1",
    "webpack": "^5.60.0"
  },
  "jest": {
    "globals": {
      "baseUrl": ""
    },
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/resources/assets/vue/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "testRegex": "tests/vue/.*.spec\\.(js|ts)$",
    "testResultsProcessor": "jest-junit",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.vue$": "<rootDir>/node_modules/jest-vue-preprocessor",
      ".*\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(vue-modal-dialogs|vuex-i18n))"
    ]
  },
  "jest-junit": {
    "ancestorSeparator": " â€º ",
    "suiteName": "Jest Tests",
    "output": "./tests-jest.xml"
  }
}
